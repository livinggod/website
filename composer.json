{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "advoor/nova-editor-js": "^2.0",
        "artesaos/seotools": "^0.20.0",
        "bakerkretzmar/nova-settings-tool": "^1.1",
        "based/laravel-fluent": "^0.0.6",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "davidhsianturi/blade-bootstrap-icons": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kabbouchi/nova-impersonate": "^1.7",
        "larabug/larabug": "dev-master",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.6",
        "laravel/nova": "~3.0",
        "laravel/octane": "^0.3.2",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "livewire/livewire": "^2.4",
        "optimistdigital/nova-translatable": "^1.9",
        "quintenbuis/laravel-instant-login": "^1.0",
        "sloveniangooner/locale-anywhere": "@dev",
        "spatie/image": "^1.10",
        "spatie/laravel-backup": "^6.15",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-disable-floc": "^0.0.2",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-ray": "^1.8",
        "spatie/laravel-robots-middleware": "^1.3",
        "spatie/laravel-sitemap": "^6.0",
        "spatie/laravel-sluggable": "^3.0",
        "spatie/laravel-translatable": "^5.0",
        "spatie/nova-backup-tool": "^4.1",
        "vink/nova-cache-card": "^1.0",
        "vyuldashev/nova-permission": "^2.10",
        "whitecube/nova-flexible-content": "^0.2.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "brianium/paratest": "^6.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0",
        "pestphp/pest": "^1.14",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-stubs": "^2.2"
    },
    "repositories": {
        "laravel/nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "sloveniangooner/locale-anywhere": {
            "type": "git",
            "url": "https://github.com/quintenbuis/locale-anywhere.git"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
